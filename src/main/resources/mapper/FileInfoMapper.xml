<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xing.chatroomapi.mapper.FileInfoMapper">

    <select id="selectByUserIdAndPidGroup" resultType="com.xing.chatroomapi.pojo.dto.FileDTO">
        select file_id,user_id,file_pid,file_name,create_time,file_category,del_flag
        from file_info
        where
        user_id = #{user}
        and
        file_pid = #{pid}
    </select>
</mapper>